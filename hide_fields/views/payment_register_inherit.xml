<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_payment_register_form_inherit" model="ir.ui.view">
        <field name="name">view.account.payment.register.form.inherit</field>
        <field name="model">account.payment.register</field>
        <field name="inherit_id" ref="view_account_payment_register_form"/>
        <field name="arch" type="xml">

            <xpath expr="//group[@name='group1']" position="after">
                <field name="reciboInterno"/>
                <field name="noLiquidacion"/>
                <field name="noDeposito"/>
            </xpath>

        </field>
    </record>

</odoo>