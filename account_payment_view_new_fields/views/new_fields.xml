<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="account_payment_view_new_fields" model="ir.ui.view">
     <field name="name">account.payment.view.new.fields</field>
     <field name="model">account.payment</field>
     <field name="inherit_id" ref="account.view_account_payment_form"/>
     <field name="arch" type="xml">

         <xpath expr="//field[@name='ref']" position="after">
             <field  name="no_recibo_interno" />
             <field  name="no_de_liquidacion" />
             <field  name="deposito_bancario" />
         </xpath>

     </field>
 </record>

</odoo>